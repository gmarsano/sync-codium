{
  "git.ignoreMissingGitWarning": true,
  "editor.fontFamily": "'Fira Code', 'Operator Mono'",
  "editor.fontLigatures": "'ss02', 'ss03', 'ss05', 'ss06'",
  "editor.lineHeight": 22,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.tabSize": 4,
  "editor.wordWrap": "on",
  "editor.rulers": [80],
  "terminal.integrated.rendererType": "canvas",
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#ff404042",
    "editorCursor.foreground": "#00aaff"
  },
  "window.zoomLevel": 1,
  "breadcrumbs.enabled": true,
  "extensions.ignoreRecommendations": false,
  "window.openFoldersInNewWindow": "on",
  "files.autoGuessEncoding": false,

  "indenticator.inner.showHighlight": true,
  "indenticator.width": 2,
  "indenticator.hover.peekBack": 1,
  "indenticator.color.dark": "#E66",
  "indenticator.color.light": "#F66",
  "indenticator.inner.color.light": "#66F",
  "indenticator.inner.color.dark": "#66E",

  "sync.forceUpload": false,

  "C_Cpp.updateChannel": "Insiders",
  
  "markdown-pdf.executablePath": "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe",
  "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; margin-left: 1cm;\"> <span class='title'></span></div> <div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; display: none; \"> <span class='date'></span></div>",

  // start bash, not the mintty, or you'll get a new window
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  // Use this to keep bash from doing a 'cd ${HOME}'
 /*  "terminal.integrated.env.windows": {
    "CHERE_INVOKING": "1"
  }, */
  // Make it a login shell
  "terminal.integrated.shellArgs.windows": [
    "-l"
  ],
  // end

  // Shell cywin64 login
  "shellLauncher.shells.windows": [
    {
      "shell": "C:/cygwin64/bin/bash.exe",
      "args": ["-l"],
      "env": {"CHERE_INVOKING": "1"},
      "label": "cywin bash"
    },
    {
      "shell": "C:\\Program Files\\Git\\bin\\bash.exe",
      "args": ["-l"],
      // "env": {"CHERE_INVOKING": "1"},
      "label": "git bash"
    },
    {
      "shell": "C:\\Windows\\System32\\cmd.exe",
      "label": "cmd"
    }
  ],

  // python
  "[python]": {
    "editor.tabSize": 4,
  },
  // pylint
  "python.linting.pylintArgs": [
    "--max-line-length=79",
    "--load-plugin","pylint_protobuf"
  ],
  // "python.linting.pycodestyleArgs": [
  // ],
  // autopep8
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": [
    "--max-line-length", "79",
    "-a", "-a"
  ],

